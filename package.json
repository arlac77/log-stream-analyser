{
  "name": "log-stream-analyser",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/log-stream-analyser.mjs",
  "module": "src/log-stream-analyser.mjs",
  "description": "split log stream into events",
  "keywords": [
    "log"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "log-stream": "bin/log-stream.mjs"
  },
  "scripts": {
    "prepare": "rollup -c",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/log-stream-analyser.mjs --section=API",
    "lint": "documentation lint src/log-stream-analyser.mjs"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "ava": "^3.5.1",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "rollup": "^2.1.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/log-stream-analyser.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/log-stream-analyser/issues"
  },
  "homepage": "https://github.com/arlac77/log-stream-analyser#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
