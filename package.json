{
  "name": "log-stream-analyser",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/log-stream-analyser.js",
  "module": "src/log-stream-analyser.mjs",
  "description": "split log stream into events",
  "keywords": [
    "log"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "log-stream": "bin/log-stream.mjs"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/log-stream-analyser.mjs --section=API",
    "lint": "documentation lint src/log-stream-analyser.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.0.2",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/log-stream-analyser.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/log-stream-analyser/issues"
  },
  "homepage": "https://github.com/arlac77/log-stream-analyser#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "js",
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
