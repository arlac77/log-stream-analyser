    "lint": "documentation lint src/log-stream-analyser.mjs",
    "prepare": "rollup -c",
    "test": "ava --timeout 2m"
  },
  "devDependencies": {
    "ava": "^3.3.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
