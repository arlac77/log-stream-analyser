{
  "name": "log-stream-analyser",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "src/log-stream-analyser.mjs",
  "description": "split log stream into events",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "log-stream-cli": "bin/log-stream"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/log-stream-analyser.mjs --section=API",
    "lint": "documentation lint src/log-stream-analyser.mjs",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.0-beta.6",
    "documentation": "^8.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.2",
    "rollup": "^0.61.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-executable": "^1.2.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.6.0",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/log-stream-analyser.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/log-stream-analyser/issues"
  },
  "homepage": "https://github.com/arlac77/log-stream-analyser#readme",
  "ava": {
    "files": [
      "build/*-test.mjs"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.mjs"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
